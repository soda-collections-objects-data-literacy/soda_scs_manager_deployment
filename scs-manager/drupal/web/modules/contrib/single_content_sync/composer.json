        "drupal/block_field": "*",
        "drupal/color_field": "*",
        "drupal/geofield": "*",
        "drupal/geolocation": "*",
        "drupal/focal_point": "*",
        "drupal/weight": "*",
        "drupal/yearonly": "*",
        "drupal/paragraphs": "*",
        "drupal/paragraph_view_mode": "*",
